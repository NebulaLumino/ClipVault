{
  "name": "clipvault",
  "version": "1.0.0",
  "description": "Automatic Gaming Highlight Delivery Bot for Discord",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:studio": "prisma studio"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/NebulaLumino/ClipVault.git"
  },
  "keywords": [
    "discord",
    "bot",
    "gaming",
    "highlights",
    "clips",
    "allstar"
  ],
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/NebulaLumino/ClipVault/issues"
  },
  "homepage": "https://github.com/NebulaLumino/ClipVault#readme",
  "dependencies": {
    "@discordjs/rest": "^2.2.0",
    "@fastify/cookie": "^9.3.1",
    "@fastify/cors": "^9.0.1",
    "@fastify/oauth2": "^7.8.0",
    "@prisma/client": "^5.10.2",
    "bullmq": "^5.4.2",
    "discord.js": "^14.14.1",
    "dotenv": "^16.4.5",
    "fastify": "^4.26.2",
    "ioredis": "^5.3.2",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/node": "^20.11.25",
    "@typescript-eslint/eslint-plugin": "^7.1.1",
    "@typescript-eslint/parser": "^7.1.1",
    "@vitest/coverage-v8": "^1.6.1",
    "eslint": "^8.57.0",
    "prettier": "^3.2.5",
    "prisma": "^5.10.2",
    "tsx": "^4.7.1",
    "typescript": "^5.4.2",
    "vitest": "^1.3.1"
  }
}
