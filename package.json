{
  "name": "clipvault",
  "version": "1.0.0",
  "description": "Automatic Gaming Highlight Delivery Bot for Discord",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "dev:web": "next dev",
    "build": "tsc",
    "build:web": "next build",
    "build:vercel": "next build",
    "start": "node dist/index.js",
    "start:web": "next start",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:studio": "prisma studio"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/NebulaLumino/ClipVault.git"
  },
  "keywords": [
    "discord",
    "bot",
    "gaming",
    "highlights",
    "clips",
    "allstar"
  ],
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/NebulaLumino/ClipVault/issues"
  },
  "homepage": "https://github.com/NebulaLumino/ClipVault#readme",
  "dependencies": {
    "@discordjs/rest": "^2.2.0",
    "@fastify/cookie": "^9.3.1",
    "@fastify/cors": "^9.0.1",
    "@fastify/oauth2": "^7.8.0",
    "@prisma/client": "^5.10.2",
    "@radix-ui/react-accordion": "^1.2.12",
    "@radix-ui/react-avatar": "^1.1.11",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-progress": "^1.1.8",
    "@radix-ui/react-slot": "^1.2.4",
    "@radix-ui/react-tabs": "^1.1.13",
    "bullmq": "^5.4.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "discord.js": "^14.14.1",
    "dotenv": "^16.4.5",
    "fastify": "^4.26.2",
    "ioredis": "^5.3.2",
    "lucide-react": "^0.575.0",
    "next": "^16.1.6",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "tailwind-merge": "^3.5.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/node": "^20.11.25",
    "@types/react": "^19.2.14",
    "@types/react-dom": "^19.2.3",
    "@typescript-eslint/eslint-plugin": "^7.1.1",
    "@typescript-eslint/parser": "^7.1.1",
    "@vitest/coverage-v8": "^1.6.1",
    "autoprefixer": "^10.4.27",
    "eslint": "^8.57.0",
    "postcss": "^8.5.6",
    "prettier": "^3.2.5",
    "prisma": "^5.10.2",
    "tailwindcss": "^3.4.17",
    "tailwindcss-animate": "^1.0.7",
    "tsx": "^4.7.1",
    "typescript": "^5.9.3",
    "vitest": "^1.3.1"
  }
}
